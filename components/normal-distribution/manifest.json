{
  "main": "pct-normal-distribution.min.js",
  "version": "1",
  "sheetSettings": {
    "defaultCols": 6,
    "defaultRows": 5,
    "minCols": 6,
    "minRows": 5
  },
  "reportSettings": {
    "defaultCols": 6,
    "defaultRows": 5,
    "minCols": 6,
    "minRows": 5
  },
  "inputs": [
    {
      "key": "header",
      "type": "Group",
      "label": "Info",
      "children": [
        {
          "key": "title",
          "type": "String",
          "label": "Title",
          "placeholder": "Leave empty for no title"
        },
        {
          "key": "subtitle",
          "type": "String",
          "label": "Subtitle",
          "placeholder": "Leave empty for no subtitle"
        }
      ]
    },
    {
      "key": "dataSource",
      "type": "Group",
      "label": "Input",
      "summary": {
        "label": "Data Source"
      },
      "children": [
        {
          "key": "metric",
          "type": "RawMetric",
          "label": "Metric",
          "allowedTypes": ["int", "float", "bool"],
          "placeholder": "Enter the metric to display",
          "required": true
        }
      ]
    },
    {
      "key": "confidenceLevelPercentage",
      "type": "Number",
      "label": "Confidence Level Percentage",
      "placeholder": "Enter the confidence level",
      "defaultValue": 95,
      "required": true
    },
    {
      "key": "ignoreZero",
      "type": "Checkbox",
      "label": "Ignore Zero",
      "defaultValue": false,
      "required": true
    }
  ]
}
